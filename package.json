{
  "name": "ssc",
  "version": "1.0.0",
  "author": "Erik",
  "license": "GPL-2.0-or-later",
  "main": "build/index.js",
  "files": [
    "build/*",
    "tools/*",
    "ssc.php",
    "LICENSE.md",
    "readme.txt"
  ],
  "keywords": [
    "wordpress",
    "gutenberg",
    "editor",
    "blocks"
  ],
  "scripts": {
    "build": "wp-scripts build src/ssc-editor.js src/ssc.js --output-path=build",
    "start": "wp-scripts start src/ssc-editor.js src/ssc.js --output-path=build",
    "format": "wp-scripts format",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "packages-update": "wp-scripts packages-update",
    "plugin-zip": "wp-scripts plugin-zip"
  },
  "dependencies": {
    "@codemirror/commands": "^6.0.1",
    "@codemirror/lang-css": "^6.0.0",
    "@codemirror/lang-json": "^6.0.0",
    "@codemirror/lint": "^6.0.0",
    "@codemirror/state": "^6.1.1",
    "@codemirror/view": "^6.0.3",
    "@dnd-kit/core": "^6.0.3",
    "@dnd-kit/sortable": "^7.0.0",
    "@dnd-kit/utilities": "^3.2.0",
    "@wordpress/scripts": "^23.6.0",
    "@wordpress/block-editor": "^9.6.0",
    "@wordpress/components": "^19.16.0",
    "@wordpress/compose": "^5.12.0",
    "@wordpress/element": "^4.12.0",
    "@wordpress/i18n": "^4.14.0",
    "animate.css": "^4.1.1",
    "animejs": "^3.2.1",
    "codemirror": "^6.0.1",
    "scrollmagic": "^2.0.7",
    "scrollmagic-plugins": "^1.0.8"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.9.0 <7"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@wordpress/eslint-plugin/recommended"
    ]
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ]
}
